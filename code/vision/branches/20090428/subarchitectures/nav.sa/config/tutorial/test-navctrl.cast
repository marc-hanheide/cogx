HOST 127.0.0.1

COMPONENT CPP laser.server LaserServerPlayer --log
COMPONENT CPP robot.server RobotbaseServerPlayer --log

SUBARCHITECTURE nav.sa
CPP WM SubarchitectureWorkingMemory  #--log --debug
CPP TM AlwaysPositiveTaskManager
CPP MG trans.process TranslationProcess --log

CPP GD display.process DisplayNavInPB --retry-interval 10 -c subarchitectures/nav.sa/config/cogxp3.ccf --log

CPP GD slam.process SlamProcess -c subarchitectures/nav.sa/config/cogxp3.ccf --max-scan-rate 5 --log -m tmpmap.metric

CPP GD navgraph.process NavGraphProcess -c subarchitectures/nav.sa/config/cogxp3.ccf --log -m tmpmap.graph

CPP GD navctrl.process NavControl -c subarchitectures/nav.sa/config/cogxp3.ccf --no-x-window --log

CPP GD peekabot.control PeekabotControl --retry-interval 10 -c subarchitectures/nav.sa/config/cogxp3.ccf --log

