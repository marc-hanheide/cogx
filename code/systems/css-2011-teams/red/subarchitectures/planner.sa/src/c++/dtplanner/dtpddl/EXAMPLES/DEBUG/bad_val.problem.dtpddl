(define (problem cogxtask)

(:domain dt-find-objects)

(:objects  dora - robot
           c3b c1b c2b - cone
           book - label
           p2b p1a p1b p2a - place
           visualobject1 - visualobject
)

(:init  (is-in dora p1a)
        (in-room c3b rb)
        (in-room p2b rb)
        (in-room c1b rb)
        (connected p1a p2a)
        (connected p2a p1b)
        (connected c1b p1b)
        (connected c2b p1b)
        (connected c3b p2b)
        (connected p1b p2b)
        (label visualobject1 book)
        (in-room c2b rb)
        (label visualobject4 cornflakes)
        (in-room p1b rb)
;; 	(ex-in-room cornflakes rb true)
;; 	(probabilistic 0.0001 (ex-in-room cornflakes rb true))
        (probabilistic  0.2000  (and  (probabilistic  0.1000  (and  
                                                                    (probabilistic  0.1500  (is-in visualobject4 c3b)
                                                                                    0.6000  (is-in visualobject4 c1b)
                                                                                    0.2000  (is-in visualobject4 c2b)
                                                                    )
                                                              )
                                                      0.9000  (ex-in-room cornflakes rb false)
                                      )
                                      (probabilistic  0.3000  (and  (ex-in-room book rb true)
                                                                    (probabilistic  0.1500  (is-in visualobject1 c3b)
                                                                                    0.6000  (is-in visualobject1 c1b)
                                                                                    0.2000  (is-in visualobject1 c2b)
                                                                    )
                                                              )
                                                      0.7000  (ex-in-room book rb false)
                                      )
                                )
                        0.2000  (and  (probabilistic  0.1000  (and  (ex-in-room cornflakes rb true)
                                                                    (probabilistic  0.1500  (is-in visualobject4 c3b)
                                                                                    0.6000  (is-in visualobject4 c1b)
                                                                                    0.2000  (is-in visualobject4 c2b)
                                                                    )
                                                              )
                                                      0.9000  (ex-in-room cornflakes rb false)
                                      )
                                      (probabilistic  0.8000  (and  (ex-in-room book rb true)
                                                                    (probabilistic  0.1500  (is-in visualobject1 c3b)
                                                                                    0.6000  (is-in visualobject1 c1b)
                                                                                    0.2000  (is-in visualobject1 c2b)
                                                                    )
                                                              )
                                                      0.2000  (ex-in-room book rb false)
                                      )
                                )
                        0.6000  (and  (category rb kitchen)
                                      (probabilistic  0.8000  (and  (ex-in-room cornflakes rb true)
                                                                    (probabilistic  0.1500  (is-in visualobject4 c3b)
                                                                                    0.6000  (is-in visualobject4 c1b)
                                                                                    0.2000  (is-in visualobject4 c2b)
                                                                    )
                                                              )
                                                      0.2000  (ex-in-room cornflakes rb false)
                                      )
                                      (probabilistic  0.2000  (and  (ex-in-room book rb true)
                                                                    (probabilistic  0.1500  (is-in visualobject1 c3b)
                                                                                    0.6000  (is-in visualobject1 c1b)
                                                                                    0.2000  (is-in visualobject1 c2b)
                                                                    )
                                                              )
                                                      0.8000  (ex-in-room book rb false)
                                      )
                                )
        )
)

(:goal  (and  ))
(:metric maximize (reward ))

)
