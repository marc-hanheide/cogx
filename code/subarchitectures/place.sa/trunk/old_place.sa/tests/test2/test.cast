HOST localhost

COMPONENT CPP Hardware.RobotbaseServer RobotbaseServer -t logfile -T cure -L subarchitectures/place.sa/tests/test2/odometry.tdf
COMPONENT CPP Hardware.LaserServer LaserServer -t logfile -T cure -L subarchitectures/place.sa/tests/test2/scans.tdf

SUBARCHITECTURE place.sa
CPP WM PlaceWorkingMemory
CPP TM AlwaysPositiveTaskManager 
CPP GD Place.DataProvider PlaceDataProvider --log true --debug true --config subarchitectures/place.sa/tests/test2/test.cfg
CPP GD Place.DataSaver PlaceDataSaver --log true --debug true
CPP MG Place.Tester PlaceTester --log true --debug true --test 2
CPP UM Hardware.VideoServer VideoServer -i 0 -f subarchitectures/place.sa/tests/test2/image.jpg -s 1 -e 1 -t 50

CONNECTION Laser::Scan Hardware.LaserServer PUSH_TO Place.DataProvider
CONNECTION Robotbase::Odometry Hardware.RobotbaseServer PUSH_TO Place.DataProvider
CONNECTION Vision::ImageFrame Place.DataProvider PULL_FROM Hardware.VideoServer

