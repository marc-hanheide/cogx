HOST localhost

# live images
#COMPONENT CPP VideoServer OpenCvLiveServer --videoname video --camids 0 --devnums 0

# reading stored images
COMPONENT CPP VideoServer OpenCvImgSeqServer --videoname video --camids 0 --files "xdata/videoseq/SwGreenTea/image_%d.jpg" --start 0 --end 15 --framerate_ms 100

SUBARCHITECTURE vision.sa
CPP WM SubarchitectureWorkingMemory
CPP TM AlwaysPositiveTaskManager
CPP MG viewer VideoViewer --videohost localhost --videoname video --camid 0

CPP MG recognizer ObjectRecognizer --videohost localhost --videoname video --log --testmode 2 --models "SwGreenTea" --modeldir xdata/models --matcher-cuda
