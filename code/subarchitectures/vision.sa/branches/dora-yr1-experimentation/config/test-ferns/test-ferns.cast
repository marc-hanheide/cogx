HOST localhost


SUBARCHITECTURE vision.sa 
CPP WM SubarchitectureWorkingMemory
CPP TM AlwaysPositiveTaskManager

# reading stored images
CPP MG videoserver OpenCvImgSeqServer --camids 0 --files "subarchitectures/vision.sa/config/test-ferns/tea%03d.jpg" --start 0 --end 3 --framerate_ms 250

# live images
#CPP MG videoserver OpenCvLiveServer --camids 0 --devnums 0

CPP MG VideoClient ObjectDetectorFERNS --videoname videoserver --camid 0 --models "subarchitectures/vision.sa/config/test-ferns/tea.jpg subarchitectures/vision.sa/config/test-ferns/poster.jpg" --mode DETECT_ONLY --displaylevel 2 #--log

CPP MG Dummy DummyDriver --labels "tea poster" --log


SUBARCHITECTURE nav.sa 
CPP WM SubarchitectureWorkingMemory
CPP TM AlwaysPositiveTaskManager
