(:action  cancel
          :precondition  (not (done ))
          :effect  (and  (done )
                         (assign (reward ) 0)
                   )
)

(:action  commit-rock_type-r2
          :parameters  (?val - geology)
          :precondition  (not (done ))
          :effect  (and  (when  (rock_type r2 ?val)
                                (assign (reward ) 300)
                         )
                         (when  (not (rock_type r2 ?val))
                                (assign (reward ) -300)
                         )
                         (committed-rock_type r2)
                         (done )
                   )
)

