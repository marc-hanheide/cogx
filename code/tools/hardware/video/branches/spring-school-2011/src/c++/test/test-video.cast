# HOST <hostname> -- default host to run all the components on, not
# required if specified elsewhere
HOST localhost

SUBARCHITECTURE vision.sa
CPP WM SubarchitectureWorkingMemory
CPP TM AlwaysPositiveTaskManager
# reading stored images
CPP MG videoserver OpenCvImgSeqServer --camids 0 --files "subarchitectures/vision.sa/config/test-ferns/tea%03d.jpg" --start 0 --end 3 --framerate_ms 250 --log

# live images
#CPP MG VideoServer OpenCvLiveServer --camids 0 --devnums 0 --log

COMPONENT CPP videoclient TestVideoClient --videoname videoserver --camids 0 --log

