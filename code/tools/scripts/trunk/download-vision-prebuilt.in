#accept="--accept .so,.h,.txt"
dirmode="--no-directories --no-host-directories"
#wget --no-parent -r -l0 $dirmode $accept --directory-prefix $dest/$arch $src
wget $dirmode --directory-prefix $dest/$arch $src
