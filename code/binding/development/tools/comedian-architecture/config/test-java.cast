HOST localhost

# SUBARCHITECTURE <subarch name> <default host to be used>
SUBARCHITECTURE stage.subarch
JAVA WM comedyarch.subarchitectures.stage.StageWorkingMemory  #--log #--debug
JAVA TM cast.architecture.subarchitecture.AlwaysPositiveTaskManager #--log
#JAVA TM comedyarch.subarchitectures.stage.StageTaskManager --log
JAVA DD straight.man comedyarch.subarchitectures.stage.StraightMan -s "what's brown and sticky" #--debug
JAVA GD funny.man comedyarch.subarchitectures.stage.FunnyMan -p "a stick" --log #--debug


# SUBARCHITECTURE <subarch name> <default host to be used>
SUBARCHITECTURE audience.subarch #irobot1
JAVA WM comedyarch.subarchitectures.audience.AudienceWorkingMemory #--log --debug
JAVA TM cast.architecture.subarchitecture.AlwaysPositiveTaskManager #--log
JAVA GD audience.member comedyarch.subarchitectures.audience.AudienceMember --reaction "YAY!"


SUBARCHITECTURE director.subarch
#in this case another wm is not necesasary
#it just needs the correct ontology 
JAVA WM comedyarch.subarchitectures.stage.StageWorkingMemory #--log --debug
JAVA TM cast.architecture.subarchitecture.AlwaysPositiveTaskManager #--log
JAVA GD ass.director comedyarch.subarchitectures.director.AssistantDirector #--log #--debug
JAVA GD director comedyarch.subarchitectures.director.Director --test --log  #--debug

