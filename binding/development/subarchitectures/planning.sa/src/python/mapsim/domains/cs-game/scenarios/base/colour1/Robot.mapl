(define (problem cs-task)
(:domain cs-game)
(:objects
  Robot - robot
  fl1 - waypoint
  fl2 - waypoint
 
  target - waypoint
  start - waypoint
 
  flag1 - flag
  flag2 - flag
  
  sqr - block
)

(:init
 
  (game colour)

  (pos flag1 : fl1)
  (pos flag2 : fl2)

  (pos sqr : start)

  (shape sqr : square)



)
(:goal 


(forall (?object - movable) 
(exists (?flag - flag)
  (and (near ?object ?flag))  
)
)
))
