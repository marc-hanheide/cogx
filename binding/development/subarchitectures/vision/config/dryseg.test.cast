# HOST <hostname> -- default host to run all the components on, not
# required if specified elsewhere
HOST  localhost

# SUBARCHITECTURE <subarch name> <default host to be used>
SUBARCHITECTURE vision.subarch #localhost

CPP WM TestWorkingMemory #--log true
CPP TM AlwaysPositiveTaskManager

#CPP UM video.server VideoServer  -o /dev/video0 --port 50000
CPP UM video.server VideoServer -f "../../data/test.vid.003/image-0.%04d.ppm" -s 0 -e 336 -t 50 --downsample 1 --port 51000

CPP MG roi.detector Segmentor -s 0.9 -o 0.5 --videohost localhost --videoport 51000 #-c 2000

CPP GD test VisionLrnTester  --log --test dryseg_test --debug true #  -- exit true --log true --debug true

CONNECTION Vision::ImageFrame roi.detector PULL_FROM video.server

