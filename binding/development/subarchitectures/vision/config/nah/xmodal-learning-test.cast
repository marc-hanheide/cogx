HOST donald

SUBARCHITECTURE vision.sa
CPP WM VisualWorkingMemory #--log
CPP TM AlwaysPositiveTaskManager
# the left Sony camera of the B21 stereo head
irobot1 CPP GD cam.server.0 CameraServer --calib subarchitectures/vision/config/camera/sony0.cal --camnum 0 --eyepose subarchitectures/vision/config/camera/ground.pose --headnum 0 --downsample 2 --log
irobot1 CPP DD video.server.0 VideoServer -o 0 --port 50031 --downsample 2
CPP MG change.detector ChangeDetector --videohost irobot1 --videoport 50031 --log #--debug
CPP MG roi.detector Segmentor -t 15 -s 0.5 -o 0.5 -a 600 --videohost irobot1 --videoport 50031 --log #--debug
CPP MG feature.extractor FeatureExtractor  --log true
CPP MG learner.recogniser Recogniser   --log true  --debug true --matWin true #--loadAV subarchitectures/vision/src/matlab/mAVs.mat
CPP MG vision.bindmonitor VisionBindingMonitor -bsa binding.sa --log true # --debug true

SUBARCHITECTURE binding.sa 
CPP  WM BindingWorkingMemory
CPP  TM AlwaysPositiveTaskManager
CPP  GD binding.scorer BindingScorer -nth 0 -maxN 1
CPP  GD binding.judge BindingJudge
CPP  GD binding.binder Binder
CPP  GD binding.groups BindingGroupManager
CPP  GD binding.state BindingStatusMonitor --log
CPP  GD binding.dot BindingDotViewer -d ./dotfiles --log --proxy short --union short



SUBARCHITECTURE comsys.sa 
JAVA WM             org.cognitivesystems.comsys.workingmemory.ComsysWorkingMemory --log true
JAVA TM             org.cognitivesystems.comsys.general.ComsysTaskManager --file ./subarchitectures/comsys.mk4/config/test-suite/general-TM.sync #--log true
JAVA GD speech.rec  org.cognitivesystems.comsys.components.asr.ASR --src gui #--log true --playback
JAVA GD parser      org.cognitivesystems.comsys.components.UtteranceInterpretation --grammar ./subarchitectures/comsys.mk4/grammars/openccg/moloko.v5/grammar.xml --log true
JAVA GD plfviz      org.cognitivesystems.comsys.components.PackedLFVisualizer --dir ./subarchitectures/comsys.mk4/graphs/parser/ --series true --sdrs true --dirSDRS ./subarchitectures/comsys.mk4/graphs/sdrs/ # --log true
JAVA GD eventInt       org.cognitivesystems.comsys.components.EventStructureInterpretation --incrementalInterpretation false #--log true
JAVA GD discRefBindings org.cognitivesystems.comsys.components.DiscRefBindingsComponent #--log true
JAVA GD dialInt org.cognitivesystems.comsys.components.DialInt #--log true
JAVA GD comsys.binding.monitor  org.cognitivesystems.comsys.monitors.ComSysBindingMonitor -bsa binding.sa --incrementalBinding false --hypoProxies false --unionSizeMin 2 --syncModel sync_one --log true


