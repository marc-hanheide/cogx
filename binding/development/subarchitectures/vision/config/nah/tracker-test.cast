HOST donald

SUBARCHITECTURE vision.sa

irobot1 CPP WM VisualWorkingMemory #--log true
irobot1 CPP TM AlwaysPositiveTaskManager

irobot1 CPP GD cam.server.0 CameraServer --calib subarchitectures/vision/config/camera/sony0.cal --camnum 0 --eyepose subarchitectures/vision/config/camera/ground.pose --headnum 0 --downsample 2 --log
irobot1 CPP DD video.server.0 VideoServer -o 0 --port 50031 --downsample 2 --log

irobot1 CPP MG change.detector ChangeDetector --videohost irobot1 --videoport 50031 --log true
irobot1 CPP MG segmentor Segmentor -t 15 -s 0.5 -o 0.5 -a 600 --videohost irobot1 --videoport 50031  --log #-c 2000 
#irobot1 CPP GD object.tracker ObjectTracker  -m blob -c subarchitectures/vision/config/tracker/trackblob.cfg --videohost irobot1 --videoport 50031 #--log #-v 1
irobot1 CPP GD object.tracker ObjectTracker  -m blob  --videohost irobot1 --videoport 50031 #--log #-v 1

