
HOST localhost

SUBARCHITECTURE comsys.subarch 
JAVA WM org.cognitivesystems.comsys.workingmemory.ComsysWorkingMemory #--debug --log true
JAVA TM org.cognitivesystems.comsys.general.ComsysTaskManager --file ./subarchitectures/comsys.mk3/config/ComsysTM.sync --log true
JAVA GD speech.rec org.cognitivesystems.comsys.components.asr.ASR --src gui --log false
JAVA GD parser org.cognitivesystems.comsys.components.Parser --grammar ./subarchitectures/comsys.mk3/grammars/openccg/moloko/grammar.xml --parser buCKY --log false
JAVA GD dialint org.cognitivesystems.comsys.components.DialInt --digrammar ./subarchitectures/comsys.mk3/grammars/grnetworks/dialint/grammar.xml --log false
JAVA GD comsys.binding.monitor org.cognitivesystems.comsys.monitors.ComSysBindingMonitor --log false -bsa binding.subarch

