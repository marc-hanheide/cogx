# CAST config file

IFOPTALL(%(hardware), laser)
    COMPONENT CPP laser.server LaserServerPlayer --player-host %(host:PlayerHost)
ENDIF

IFOPTALL(%(hardware), robot)
    COMPONENT CPP robot.server RobotbaseServerPlayer --player-host %(host:PlayerHost) #--log #--rand-data
ENDIF

