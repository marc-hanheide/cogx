# Learn a new object, in real.

HOST localhost

COMPONENT CPP ptz.server PlayerPTZServer --ptz-device 0 

SUBARCHITECTURE vision.sa 
CPP WM SubarchitectureWorkingMemory
CPP TM AlwaysPositiveTaskManager

CPP MG CameraMount CameraMount --camids 0 --use_ptz --cam_poses_xml "instantiations/green_css2011/calibration/cam-left/campose-left.xml" --pt_base_xml instantiations/green_css2011/calibration/ptu-pose-base-sim.xml --pt_pan_xml instantiations/green_css2011/calibration/ptu-pose-pan-sim.xml --pt_tilt_xml instantiations/green_css2011/calibration/ptu-pose-tilt-sim.xml --fixed_cam_poses_xml "instantiations/green_css2011/calibration/cam-left/campose-left.xml" --fixed_pan_tilt "0 -1.05"

CPP MG VideoServer OpenCvLiveServer --camids "0" --devclass FIREWIRE --devnums "0" --camconfigs "instantiations/green_css2011/calibration/example-cam-left-cal.xml"

CPP MG Tracker ObjectTracker --videoname VideoServer --camid 0 --textured --inifile "subarchitectures/vision.sa/config/objecttracker.ini" --maxModels 2 --log

CPP MG Recognizer3D ObjectRecognizer3D --videoname VideoServer --camid "0" --display --plyfiles "instantiations/green_css2011/object-models/example-cereals-schokomusli.ply" --siftfiles "instantiations/green_css2011/object-models/example-cereals-schokomusli.sift" --labels "example-cereals-schokomusli" --initpose "[0.50 0.00 0.57][0.00 0.00 1.50]" --log

# mode can be RECOGNIZE or LEARN
CPP MG Recognizer3DDriver ObjectRecognizer3DDriver --labels "example-cereals-schokomusli" --mode LEARN --log

JAVA MG ptzServer ptz.ptzServer.PanTiltZoomServer --testGUI
