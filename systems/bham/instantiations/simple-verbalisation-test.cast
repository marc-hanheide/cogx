HOST localhost

SUBARCHITECTURE comsys
## -------------------------------------------
## Working memory and task manager
## -------------------------------------------
JAVA WM			cast.architecture.SubarchitectureWorkingMemory #--log true --debug
JAVA TM 		cast.architecture.AlwaysPositiveTaskManager #--log --debug
JAVA GD contentPlanning comsys.components.realize.cc_ContentPlanning --grammar ./subarchitectures/comsys/grammars/contentPlanning/grammar.xml --redux ./subarchitectures/comsys/grammars/contentPlanning/modelredux.rdx #--log
JAVA GD realizer comsys.components.realize.cc_Realizer --ccg ./subarchitectures/comsys/grammars/openccg/moloko.v6/grammar.xml --log
## other voice options: hmm-slt (female hmm voice), hmm-jmk (male hmm voice), us1 (female intonation), us2 (male intonation)
JAVA GD speech.synth   comsys.components.tts.cc_TTS --startserver --serverHost localhost --serverPort 59125 --voice hmm-slt #--log true --debug
JAVA MG verb.test       dora.Verbalisation --log
