(define (problem cogx-problem)
    (:domain cogx)
	
	(:objects
        dora - robot
        p1 p2 p3 p4 p5 - place
	)

    (:init
		(connected p1 p2)
		(connected p2 p3)
		(connected p3 p4)
		(connected p4 p5)
		(connected p5 p1)
        (= (is-in dora) p1)
    )
    (:metric minimize (total-cost))
    (:goal (forall (?p - place) (visited ?p)))
)
