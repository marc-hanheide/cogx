<scene>
  <model>
    <file>sth_dcsg_var/frame.pbmf</file>
    <name>frame</name>
  </model>

  <model>
    <file>sth_dcsg_var/cam_left.pbmf</file>
    <name>cam_left</name>
    <transform>
      <translate>0 -0.08 0</translate>
    </transform>
  </model>

  <model>
    <file>sth_dcsg_var/cam_right.pbmf</file>
    <name>cam_right</name>
    <transform>
      <translate>0 0.08 0</translate>
    </transform>
  </model>
</scene>
