   "   assert(displist, \"No lists have been defined in this context\")\n"
   "   local id = displist[name]\n"
   "   assert(id, \"DispList named '\" .. name .. \"' not found\")\n"
   "   glCallList(id)\n"
   "end\n"
   "\n"
