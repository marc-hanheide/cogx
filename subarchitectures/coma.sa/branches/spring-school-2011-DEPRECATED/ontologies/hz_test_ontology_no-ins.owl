<?xml version="1.0"?>
<rdf:RDF
    xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
    xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns="http://hz.test.owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
  xml:base="http://hz.test.owl">
  <owl:Ontology rdf:about=""/>
  <owl:Class rdf:ID="Refrigerator">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="KitchenObject"/>
    </rdfs:subClassOf>
    <lexicalWord rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >refrigerator</lexicalWord>
  </owl:Class>
  <owl:Class rdf:ID="PhysicalRoom">
    <lexicalWord rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >room</lexicalWord>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Portion_of_Space"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Notebook">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="OfficeObject"/>
    </rdfs:subClassOf>
    <lexicalWord rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >notebook</lexicalWord>
    <owl:equivalentClass>
      <owl:Class rdf:ID="Laptop"/>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="Office">
    <lexicalWord rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >office</lexicalWord>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#PhysicalRoom"/>
          <owl:Restriction>
            <owl:onProperty>
              <owl:ObjectProperty rdf:ID="contains"/>
            </owl:onProperty>
            <owl:someValuesFrom>
              <owl:Class rdf:about="#OfficeObject"/>
            </owl:someValuesFrom>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="Object">
    <lexicalWord rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >object</lexicalWord>
  </owl:Class>
  <owl:Class rdf:about="#KitchenObject">
    <rdfs:subClassOf rdf:resource="#Object"/>
  </owl:Class>
  <owl:Class rdf:about="#Laptop">
    <lexicalWord rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >laptop</lexicalWord>
    <owl:equivalentClass rdf:resource="#Notebook"/>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#OfficeObject"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="FunctionEvidence"/>
  <owl:Class rdf:about="#OfficeObject">
    <rdfs:subClassOf rdf:resource="#Object"/>
  </owl:Class>
  <owl:Class rdf:about="#Portion_of_Space">
    <lexicalWord rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >place</lexicalWord>
  </owl:Class>
  <owl:Class rdf:ID="Doorway">
    <lexicalWord rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >doorway</lexicalWord>
    <rdfs:subClassOf rdf:resource="#Portion_of_Space"/>
  </owl:Class>
  <owl:Class rdf:ID="Place"/>
  <owl:Class rdf:ID="KitchenEvidence">
    <rdfs:subClassOf rdf:resource="#FunctionEvidence"/>
  </owl:Class>
  <owl:Class rdf:ID="KitchenRegion">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="FunctionalRegion"/>
    </rdfs:subClassOf>
    <lexicalWord rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >kitchen</lexicalWord>
  </owl:Class>
  <owl:Class rdf:about="#FunctionalRegion">
    <rdfs:subClassOf rdf:resource="#Portion_of_Space"/>
  </owl:Class>
  <owl:Class rdf:ID="Kitchen">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#KitchenObject"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#contains"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="#PhysicalRoom"/>
    <lexicalWord rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >kitchen</lexicalWord>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="constituentOf">
    <rdfs:domain rdf:resource="#Place"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#contains">
    <lexicalWord rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >contains</lexicalWord>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="in"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="constituentOfRegion">
    <rdfs:range rdf:resource="#FunctionalRegion"/>
    <rdfs:subPropertyOf rdf:resource="#constituentOf"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#in">
    <rdfs:domain rdf:resource="#Object"/>
    <rdfs:range rdf:resource="#Place"/>
    <lexicalWord rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >is located in</lexicalWord>
    <owl:inverseOf rdf:resource="#contains"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="constituentOfRoom">
    <rdfs:subPropertyOf rdf:resource="#constituentOf"/>
    <rdfs:range rdf:resource="#PhysicalRoom"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="providesEvidence">
    <rdfs:range rdf:resource="#FunctionEvidence"/>
    <rdfs:domain rdf:resource="#Place"/>
  </owl:ObjectProperty>
  <owl:SymmetricProperty rdf:ID="adjacent">
    <rdfs:range rdf:resource="#Place"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain rdf:resource="#Place"/>
    <owl:inverseOf rdf:resource="#adjacent"/>
  </owl:SymmetricProperty>
  <owl:SymmetricProperty rdf:ID="sameRoomAs">
    <rdfs:range rdf:resource="#Place"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <owl:inverseOf rdf:resource="#sameRoomAs"/>
    <rdfs:domain rdf:resource="#Place"/>
  </owl:SymmetricProperty>
  <owl:SymmetricProperty rdf:ID="possiblySameRoomAs">
    <owl:inverseOf rdf:resource="#possiblySameRoomAs"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="#Place"/>
    <rdfs:domain rdf:resource="#Place"/>
  </owl:SymmetricProperty>
  <owl:AnnotationProperty rdf:ID="lexicalWord">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:AnnotationProperty>
</rdf:RDF>

