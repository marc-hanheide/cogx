		   </choicenode>			
		 </choicenode>  <!-- end color is thematic -->

		 <choicenode answer = "t-rheme-true">  <!-- color is rhematic -->
           <inquiry id="f-featval" type="string" answerset="@true @false @unknown"> 
    		    <f-featval feat="KontrastColor" nomvar="ENTITY4COLOR" />
   		   </inquiry>			
		   <choicenode answer = "true">  
			  <result val="m-focus-true-rhematic"/>			
		   </choicenode>
		   <choicenode answer = "false">
			  <result val="m-focus-false-rhematic"/>
		   </choicenode>
		   <choicenode answer = "unknown">  
			  <result val="m-focus-true-rhematic"/>		<!-- Focus=true is the default in rheme -->	
		   </choicenode>			
		 </choicenode>  <!-- end color is rhematic -->

		 <choicenode answer = "unknown">               <!-- no Rheme feature present at entity: default -->
			  <result val="m-focus-false-thematic"/>
         </choicenode>

	   </choicenode>  <!-- and TOP -->
   </dectree>
</chooser>

