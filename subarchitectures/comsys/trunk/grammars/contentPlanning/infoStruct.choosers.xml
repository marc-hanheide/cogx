<chooser id="c-rheme.ownership" region="infoStruct" metafunction="textual">
   <dectree>
       <choicenode answer="*TOP*">
		 <!-- first check whether RhOwner has already been set to avoid duplication -->
         <inquiry id="q-ex-feat" type="string" answerset="@true @false"> 
		    <q-ex-feat feat="Owner"/>
		 </inquiry>			
            <choicenode answer = "true">  <!-- assigned -->
                <result val="doneRhOwner"/>
            </choicenode>

		 <choicenode answer = "false">  <!-- not assigned -->

           <inquiry id="f-featval" type="string" answerset="@assertion @question @directive @commitment @unknown"> 
                <f-featval nomvar="DVP" feat="SpeechAct"/> 
            </inquiry>
            
            <choicenode answer="assertion">   <!-- assertion: speakerRhOwner -->
              <inquiry id="f-featval" type="string" answerset="@t-rheme-true @m-rheme-true @e-rheme-true @unknown"> 
                 <f-featval feat="Rheme"/>
              </inquiry>			
              <choicenode answer = "t-rheme-true">
                <result val="speaker-t-rheme"/>
              </choicenode>
              <choicenode answer = "m-rheme-true">
                <result val="speaker-m-rheme"/>
              </choicenode>
              <choicenode answer = "e-rheme-true">
                <result val="speaker-e-rheme"/>
              </choicenode>
		      <choicenode answer = "unknown">   <!-- means Rheme is not there, should never happen! -->
                <result val="noRhemeFeature"/>
              </choicenode>
             </choicenode>  <!-- end assertion -->

            <choicenode answer="question">  <!-- question: hearerRhOwner for now -->
              <inquiry id="f-featval" type="string" answerset="@t-rheme-true @m-rheme-true @e-rheme-true @unknown"> 
                 <f-featval feat="Rheme"/>
              </inquiry>			
              <choicenode answer = "t-rheme-true">
                <result val="hearer-t-rheme"/>
              </choicenode>
              <choicenode answer = "m-rheme-true">
                <result val="hearer-m-rheme"/>
              </choicenode>
              <choicenode answer = "e-rheme-true">
                <result val="hearer-e-rheme"/>
              </choicenode>
		      <choicenode answer = "unknown">   <!-- means Rheme is not there, should never happen! -->
                <result val="noRhemeFeature"/>
              </choicenode>
			</choicenode>  <!-- end question -->

			<choicenode answer="directive">  <!-- directive: hearerRhOwner -->  
              <inquiry id="f-featval" type="string" answerset="@t-rheme-true @m-rheme-true @e-rheme-true @unknown"> 
                 <f-featval feat="Rheme"/>
              </inquiry>			
              <choicenode answer = "t-rheme-true">
                <result val="hearer-t-rheme"/>
              </choicenode>
              <choicenode answer = "m-rheme-true">
                <result val="hearer-m-rheme"/>
              </choicenode>
              <choicenode answer = "e-rheme-true">
                <result val="hearer-e-rheme"/>
              </choicenode>
