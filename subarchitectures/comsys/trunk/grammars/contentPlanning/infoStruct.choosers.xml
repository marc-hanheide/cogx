<chooser id="c-rheme.ownership" region="infoStruct" metafunction="textual">
   <dectree>
       <choicenode answer="*TOP*">
		 <!-- first check whether RhOwner has already been set to avoid duplication -->
         <inquiry id="q-ex-feat" type="string" answerset="@true @false"> 
		    <q-ex-feat feat="RhOwner" nomvar="CONTENT"/>
		 </inquiry>			
            <choicenode answer = "true">
                <result val="doneRhOwner"/>
            </choicenode>

		 <choicenode answer = "false">

           <inquiry id="f-featval" type="string" answerset="@assertion @question @directive @commitment @unknown"> 
                <f-featval nomvar="DVP" feat="SpeechAct"/> 
            </inquiry>
					<choicenode answer="assertion"> 
					  <result val="speakerRhOwner"/>
					</choicenode>
					<choicenode answer="question">  <!-- question means something info request, really -->
					  <result val="hearerRhOwner"/>
					</choicenode>
					<choicenode answer="directive">
					  <result val="hearerRhOwner"/>  
					</choicenode>
					<choicenode answer="commitment">
					   <result val="speakerRhOwner"/>
					</choicenode>
					<choicenode answer="unknown">  <!-- default -->
					  <result val="speakerRhOwner"/>
					</choicenode>
        </choicenode>
	</choicenode>
   </dectree>
</chooser>


