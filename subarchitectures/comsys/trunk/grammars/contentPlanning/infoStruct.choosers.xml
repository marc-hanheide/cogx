    		    <f-featval feat="KontrastColor" nomvar="ENTITY4COLOR" />
   		   </inquiry>			
		   <choicenode answer = "true">  
			  <result val="m-focus-true-thematic"/>			
		   </choicenode>
		   <choicenode answer = "false">
