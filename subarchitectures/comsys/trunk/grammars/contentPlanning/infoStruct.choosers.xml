<chooser id="c-focus.assignment" region="infoStruct" metafunction="textual">
   <dectree>
       <choicenode answer="*TOP*">
       
         <inquiry id="q-ex-feat" type="string" answerset="@true @false"> 
		    <q-ex-feat feat="Focus" />  <!-- check if Focus already assigned -->
 		 </inquiry>	
        <choicenode answer = "true">           <!-- is assigned -->       
            <result val="doneFocus"/>
        </choicenode>

        <choicenode answer = "false">           <!-- is not assigned -->      
         <inquiry id="f-featval" type="string" answerset="@true @false @unknown"> 
		    <f-featval feat="Kontrast" />  <!-- check explicit Kontrast feature in input LF -->
 		 </inquiry>			
 
            <choicenode answer = "true">           <!-- is Kontrastive -->
                <inquiry id="f-featval" type="string" answerset="@t-rheme-false @m-rheme-false @e-rheme-false @t-rheme-true @m-rheme-true @e-rheme-true @unknown"> 
                <f-featval feat="Rheme"/>
            </inquiry>			
                <choicenode answer = "t-rheme-false">
                    <result val="t-focus-true"/>
                </choicenode>

                    <choicenode answer = "m-rheme-false">
                        <result val="m-focus-true"/>
                    </choicenode>

                <choicenode answer = "e-rheme-false">
                    <result val="e-focus-true"/>
                </choicenode>

                <choicenode answer = "t-rheme-true">
                    <result val="t-focus-true"/>
                </choicenode>

                <choicenode answer = "m-rheme-true">
                    <result val="m-focus-true"/>
                </choicenode>

                <choicenode answer = "e-rheme-true">
                    <result val="e-focus-true"/>
                </choicenode>

                <choicenode answer = "unknown">   <!-- means Rheme is not there, should never happen! -->
                    <result val="noRhemeFeature"/>
                </choicenode>
            </choicenode> <!-- end is Kontrastive -->

            <choicenode answer = "false">           <!-- is not Kontrastive -->
                <inquiry id="f-featval" type="string" answerset=
                                       "@t-rheme-false @m-rheme-false @e-rheme-false @t-rheme-true @m-rheme-true @e-rheme-true @unknown"> 
                <f-featval feat="Rheme"/>
            </inquiry>			
                <choicenode answer = "t-rheme-false">
                    <result val="t-focus-false"/>
                </choicenode>

                    <choicenode answer = "m-rheme-false">
                        <result val="m-focus-false"/>
                    </choicenode>

                <choicenode answer = "e-rheme-false">
                    <result val="e-focus-false"/>
                </choicenode>

                <choicenode answer = "t-rheme-true">
                    <result val="t-focus-false"/>
                </choicenode>

                <choicenode answer = "m-rheme-true">
                    <result val="m-focus-false"/>
                </choicenode>

                <choicenode answer = "e-rheme-true">
                    <result val="e-focus-false"/>
                </choicenode>

                <choicenode answer = "unknown">   <!-- means Rheme is not there, should never happen! -->
                    <result val="noRhemeFeature"/>
                </choicenode>
            </choicenode> <!-- end is not Kontrastive -->

            <choicenode answer = "unknown">           <!-- no Kontrast feature: assign focus by default -->
              <!-- do some default magic later 
                    if noun-modifier then no focus (done in rhemeAssign, first need to stop it there)
                    if verb then no focus 
                    if Thematic then no focus (done)
                    if "I" then no focus (done)
                    if the only (rhematic) element assign focus how?
                    focus type/color/shape in answers according to QuestionType
                     -->

            <inquiry id="q-ex-prop" type="string" answerset="@true @false"> 
                <q-ex-prop val="I" />  <!-- check if "I" -->
            </inquiry>	
            <choicenode answer = "true">           <!-- is "I" -->       
                <result val="t-focus-false"/>
            </choicenode>

            <choicenode answer = "false">           <!-- is not "I" -->      
            
                <inquiry id="f-featval" type="string" answerset="@t-rheme-false @m-rheme-false @e-rheme-false @t-rheme-true @m-rheme-true @e-rheme-true @unknown"> 
                <f-featval feat="Rheme"/>
            </inquiry>			
                <choicenode answer = "t-rheme-false">
                    <result val="t-focus-false"/>
                </choicenode>

                    <choicenode answer = "m-rheme-false">
                        <result val="m-focus-false"/>
                    </choicenode>

                <choicenode answer = "e-rheme-false">
                    <result val="e-focus-false"/>
                </choicenode>

                <choicenode answer = "t-rheme-true">
                    <result val="t-focus-true"/>
                </choicenode>

                <choicenode answer = "m-rheme-true">
                    <inquiry id="q-deprel-type" type="string" answerset="@true @false"> 
                        <q-deprel-type mode="Modifier" />  <!-- check if noun-modifier -->
                    </inquiry>	
                    <choicenode answer = "true">           <!-- is noun-modifier -->       
                        <result val="m-focus-false"/>
                    </choicenode>

                    <choicenode answer = "false">           <!-- is not noun modifier, so it is a property in ascription -->      
                        <result val="m-focus-true"/>
                    </choicenode>                    
                </choicenode>  <!-- end m-rheme-true -->

                <choicenode answer = "e-rheme-true">
                    <result val="e-focus-true"/>
                </choicenode>

                <choicenode answer = "unknown">   <!-- means Rheme is not there, should never happen! -->
                    <result val="noRhemeFeature"/>
                </choicenode>
            </choicenode> <!-- end no Kontrast feature -->

       </choicenode> <!-- end is not "I" -->

       </choicenode> <!-- end is not assigned -->

     </choicenode> <!-- end TOP -->
   </dectree>
</chooser>


