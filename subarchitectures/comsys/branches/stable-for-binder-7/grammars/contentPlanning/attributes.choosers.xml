<chooser id="c-attribute.NumberID" region="ATTRIBUTES" metafunction="ideational">
   <dectree>
       <choicenode answer="*TOP*">
         <inquiry id="q-ex-feat" type="string" answerset="@true @false"> 
		    <q-ex-feat feat="NumberID" />
		 </inquiry>			
		 <choicenode answer = "true">  
