<choosers>

<chooser id="c-attribute.color" region="ATTRIBUTES" metafunction="ideational">

   <dectree>
       <choicenode answer="*TOP*">
         <inquiry id="q-ex-feat" type="string" answerset="@true @false"> 
	     <q-ex-feat feat="Color"/>	                                 
	     </inquiry>
  	     <choicenode answer="true">
	       <result val="color"/>
	     </choicenode>
	     <choicenode answer="false">
	       <result val="no-color"/>
	     </choicenode>
	</choicenode>
   </dectree>
</chooser>

