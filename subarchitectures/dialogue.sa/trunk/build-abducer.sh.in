#!/bin/bash
cd external

ABDUCER_VERSION="2.0.1"

if [ "abducer-$ABDUCER_VERSION" -nt "abducer-$ABDUCER_VERSION.tar.gz" ]; then
    echo "abducer is up to date"
else
    tar xzvf "abducer-$ABDUCER_VERSION.tar.gz"
    touch "abducer-$ABDUCER_VERSION"
fi

make -C "abducer-$ABDUCER_VERSION"  ICE_HOME=@ICE_HOME@
