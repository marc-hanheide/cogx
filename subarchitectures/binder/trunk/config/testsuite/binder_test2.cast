HOST localhost

SUBARCHITECTURE binder 

JAVA TM cast.architecture.AlwaysPositiveTaskManager #--log
JAVA WM binder.components.BindingWorkingMemory #--log

JAVA MG binder binder.components.Binder --alpha 0.8f --nbestsfilter 5 --addunknowns true --bayesiannetworkfile ./subarchitectures/binder/config/bayesiannetwork.txt #--log
JAVA MG discretizer binder.components.UnionDiscretizer --onlymaxfeatvalues true #--log

JAVA MG test1 binder.tests.Test2 --nbproxies 80 --nbfeatures 3 --nbfeatvals 2 --nbsubarchs 3 #--log

JAVA MG monitor binder.components.BinderMonitor #--log
