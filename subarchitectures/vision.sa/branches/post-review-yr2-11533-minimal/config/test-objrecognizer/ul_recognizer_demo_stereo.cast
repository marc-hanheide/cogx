# CAST config file
HOST localhost

COMPONENT CPP stereoserver StereoServer --stereoname stereo --videoname VideoServer --camids "0 1" --stereoconfig subarchitectures/vision.sa/config/test-stereo/stereo.ini #--display

INCLUDE ../visualization.incl

CPP MG fileviewer.cli FileMonitorDC --displayserver "display.srv" --setvars "dir=/tmp" --monitor "SiftInput=image:%(dir)/{sift_inputimage.jpg}  Model.Best=image:%(dir)/{sift_bestmodel01.png} Model.Distribution=html:%(dir)/{modeldistrib.html}" --log true

SUBARCHITECTURE vision.sa
CPP WM SubarchitectureWorkingMemory
CPP TM AlwaysPositiveTaskManager

CPP MG VideoServer OpenCvLiveServer --camids "0 1" --devnums "0 1" --devclass FIREWIRE --camconfigs "subarchitectures/vision.sa/config/tuw_stereo_new.ini:L  subarchitectures/vision.sa/config/tuw_stereo_new.ini:R" --imgsize "640 480"

# CPP MG vis.recognizer.srv ObjectRecognizerSrv --matcher-cuda --models "MDarjeeling SwGreenTea2 ShelcoreCube2 TwLemonTea2" --modeldir "subarchitectures/vision.sa/config/test-objrecognizer/model" --log true --debug true

CPP MG vis.recognizer.srv ObjectRecognizerSrv --matcher-cuda --models "MDarjeeling SwGreenTea2 ShelcoreCube2 TwLemonTea2 JaffaCakesOrange TknWhiteTea HagenDazsBrownie PilotBL-G2-5-Blue" --modeldir "xdata/models/george640/models.db" --log

CPP MG PlanePopOut PlanePopOut --videoname VideoServer --stereohost localhost --stereoname stereoserver  # --log # --display
CPP MG Filter SOIFilter --log true --videoname VideoServer --camid 0 --stereoname stereoserver --objht 23 --objdt 60 --bght 24 --bgdt 9999 --fixc 24 --display

CPP MG vis.recognizer.test TestObjectRecognizer --testmode stereo-pipeline  --recognizerhost localhost --recognizerid vis.recognizer.srv --log true # --debug true

CPP MG vis.simulation SimulatedVision --displayserver display.srv
