HOST 192.168.1.2

SUBARCHITECTURE vision.sa 
CPP WM SubarchitectureWorkingMemory
CPP TM AlwaysPositiveTaskManager

# reading stored images
CPP MG videoserver OpenCvImgSeqServer --camids 0 --files "subarchitectures/vision.sa/config/test-ferns/tea%03d.jpg" --start 0 --end 3 --framerate_ms 250 --debug true

# live images
# CPP MG videoserver OpenCvLiveServer --camids 0 --devnums 0

CPP MG video.viewer VideoViewer --videoname videoserver --camid 0 --debug true

