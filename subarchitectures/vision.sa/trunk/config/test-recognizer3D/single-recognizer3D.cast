HOST localhost

# reading stored images

SUBARCHITECTURE vision.sa 
CPP WM SubarchitectureWorkingMemory
CPP TM AlwaysPositiveTaskManager

CPP MG VideoServer OpenCvLiveServer --camids "0" --devnums "0" --camconfigs "subarchitectures/vision.sa/config/test-recognizer3D/tm.calb"

#CPP MG VideoServer OpenCvImgSeqServer --camids 0 --files "subarchitectures/vision.sa/config/test-recognizer3D/img%03d.jpg" --start 0 --end 0 --framerate_ms 100 --camconfigs "subarchitectures/vision.sa/config/test-recognizer3D/tm.calb"

CPP MG Tracker ObjectTracker --videoname VideoServer --camid 0 --textured --inifile "subarchitectures/vision.sa/config/objecttracker.ini" --maxModels 10 --log

CPP MG Recognizer3D ObjectRecognizer3D --videoname VideoServer --camid "0" --display --plyfiles "instantiations/ply-models/TUM.ply instantiations/ply-models/JodSalz.ply" --siftfiles "instantiations/sift-models/TUM.sift instantiations/sift-models/JodSalz.sift" --labels "TUM JodSalz" --log

CPP MG Recognizer3DDriver ObjectRecognizer3DDriver --labels "TUM JodSalz" --log

CPP MG VirtualScene VirtualScene --videoname VideoServer --camid 0 --log  #--debug 


