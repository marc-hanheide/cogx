# Test
HOST localhost

SUBARCHITECTURE vision.sa
CPP WM SubarchitectureWorkingMemory
CPP TM AlwaysPositiveTaskManager

# live images
# CPP MG VideoServer OpenCvLiveServer --camids 0 --devnums 0

# reading stored images
CPP MG VideoServer OpenCvImgSeqServer --camids "0" --devnums "0" --files "subarchitectures/vision.sa/config/test-objrecognizer/video/SwGreenTea/smaller40/image_%d.jpg" --start 0 --end 15 --framerate_ms 300

CPP MG viewer VideoViewer --videohost localhost --videoname VideoServer --camid "0"

CPP MG recognizer ObjectRecognizer --videohost localhost --videoname VideoServer --models "SwGreenTea ShelcoreCube TwLemonTea" --modeldir "subarchitectures/vision.sa/config/test-objrecognizer/model" --matcher-cuda --log true --testmode 2

