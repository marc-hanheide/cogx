HOST localhost


SUBARCHITECTURE vision.sa 
CPP WM SubarchitectureWorkingMemory
CPP TM AlwaysPositiveTaskManager
# reading stored images
#CPP MG videoserver OpenCvImgSeqServer --camids 0 --camconfigs "subarchitectures/vision.sa/config/cam.cal" --files "subarchitectures/vision.sa/src/c++/vision/components/VisionDriver/img/00000%03d.jpg" --start 150 --end 400 --framerate_ms 50

# live images
CPP MG VideoServer OpenCvLiveServer --videoname video --camids "0" --devnums "1" --camconfigs "subarchitectures/vision.sa/config/cam.cal"

CPP MG ObjectDetector ObjectDetector --videoname VideoServer --camid 0 --log #--debug 
CPP MG Tracker ObjectTracker --videoname VideoServer --camid 0 --maxModels 6 --log  #--debug 
CPP MG VisionDriver VisionDriver --log #--debug 
