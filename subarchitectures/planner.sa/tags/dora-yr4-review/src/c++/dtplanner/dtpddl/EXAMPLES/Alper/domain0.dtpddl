(:action  report_position
          :parameters  (?a - robot ?o - visualobject ?p - place)
          :precondition  (and  (kval-related-to ?a ?o)
                               (is-in ?a ?p)
                         )
          :effect  (and  (position-reported ?o)
                         (assign (reward ) -1.0000)
                   )
)

(:action  move
          :parameters  (?a - robot ?to - place ?from - place)
          :precondition  (and  (or  (connected ?from ?to)
                                    (connected ?to ?from)
                               )
                               (not (done ))
                               (is-in ?a ?from)
                         )
          :effect  (and  (not (is-in ?a ?from))
                         (is-in ?a ?to)
                         (placestatus ?to trueplace)
                         (kd-in-room ?a ?to)
                         (assign (reward ) -2.0000)
                   )
)

(:action  move_direct
          :parameters  (?a - robot ?to - place ?from - place ?via - place)
          :precondition  (and  (or  (connected ?from ?via)
                                    (connected ?via ?from)
                               )
                               (or  (connected ?via ?to)
                                    (connected ?to ?via)
                               )
                               (not (done ))
                               (is-in ?a ?from)
                         )
          :effect  (and  (not (is-in ?a ?from))
                         (is-in ?a ?to)
                         (placestatus ?to trueplace)
                         (kd-in-room ?a ?to)
                         (assign (reward ) -3.0000)
                   )
)

(:action  create_cones_in_room
          :parameters  (?a - robot ?l - label ?r - room ?p - place)
          :precondition  (and  (is-in ?a ?p)
                               (in-room ?p ?r)
                               (poss-obj_exists ?l in ?r true)
                               (not (not_fully_explored ?r))
                               (not (done ))
                         )
          :effect  (and  (cones_created ?l in ?r)
                         (assign (reward ) -1.0000)
                   )
)

(:action  create_cones_at_object
          :parameters  (?a - robot ?l - label ?lsupp - label ?rel - spatial_relation ?o - visualobject ?r - room ?p - place)
          :precondition  (and  (is-in ?a ?p)
                               (label ?o ?lsupp)
                               (poss-obj_exists ?l ?rel ?o true)
                               (in-room ?p ?r)
                               (related-to ?o ?r)
                               (not (done ))
                         )
          :effect  (and  (cones_created ?l ?rel ?o)
                         (assign (reward ) -1.0000)
                   )
)

(:action  search_for_object_in_room
          :parameters  (?a - robot ?l - label ?r - room ?p - place ?o - visualobject)
          :precondition  (and  (is-in ?a ?p)
                               (in-room ?p ?r)
                               (label ?o ?l)
                               (or  (cones_created ?l in ?r)
                                    (cones_exist ?l in ?r)
                               )
                               (poss-related-to ?o ?r)
                               (poss-relation ?o in)
                               (not (done ))
                         )
          :effect  (and  (kd-related-to ?a ?o)
                         (assign (reward ) -10)
                   )
)

(:action  search_for_object_at_object
          :parameters  (?a - robot ?l - label ?o - visualobject ?rel - spatial_relation ?p - place ?r - room ?o2 - visualobject)
          :precondition  (and  (is-in ?a ?p)
                               (in-room ?p ?r)
                               (related-to ?o ?r)
                               (label ?o2 ?l)
                               (or  (cones_created ?l ?rel ?o)
                                    (cones_exist ?l ?rel ?o)
                               )
                               (poss-related-to ?o2 ?o)
                               (poss-relation ?o2 ?rel)
                               (not (done ))
                         )
          :effect  (and  (kd-related-to ?a ?o2)
                         (assign (reward ) -10)
                   )
)

(:action  process_conegroup
          :parameters  (?a - robot ?c - conegroup ?p - place)
          :precondition  (and  (not (done ))
                               (cg-place ?c ?p)
                               (is-in ?a ?p)
                         )
          :effect  (and  (assign (reward ) -15.0000))
)

(:action  cancel
          :precondition  (and  (not (done )))
          :effect  (and  (done )
                         (assign (reward ) 0)
                   )
)

(:action  commit-related-to-visualobject3-room_0__t
          :precondition  (and  (not (done )))
          :effect  (and  (when  (related-to visualobject3 room_0__t)
                                (assign (reward ) 100)
                         )
                         (when  (not (related-to visualobject3 room_0__t))
                                (assign (reward ) -65.3271)
                         )
                         (done )
                   )
)

(:action  disconfirm-category-room_0__t
          :precondition  (and  (not (done )))
          :effect  (and  (when  (not (category room_0__t kitchen))
                                (assign (reward ) 15.4098)
                         )
                         (when  (category room_0__t kitchen)
                                (assign (reward ) -21.9261)
                         )
                         (done )
                   )
)

(:action  disconfirm-obj_exists-table-in-room_0__t
          :precondition  (and  (not (done )))
          :effect  (and  (when  (not (obj_exists table in room_0__t true))
                                (assign (reward ) 80.0000)
                         )
                         (when  (obj_exists table in room_0__t true)
                                (assign (reward ) -122.4607)
                         )
                         (done )
                   )
)

(:action  disconfirm-relation-visualobject3
          :precondition  (and  (not (done )))
          :effect  (and  (when  (not (relation visualobject3 in))
                                (assign (reward ) 0.0000)
                         )
                         (when  (relation visualobject3 in)
                                (assign (reward ) 0.0000)
                         )
                         (done )
                   )
)

(:observe  visual_object
           :parameters  (?a - robot ?c - conegroup ?o - visualobject ?l - label ?where - either_visualobject_room ?p - place)
           :execution  (process_conegroup ?a ?c ?p)
           :precondition  (and  (label ?o ?l)
                                (cg-label ?c ?l)
                                (cg-related-to ?c ?where)
                          )
           :effect  (and  (when  (visible_from ?o ?c)
                                 (probabilistic  0.8000  (observed-related-to ?o ?where))
                          )
                          (when  (not (visible_from ?o ?c))
                                 (probabilistic  0.0500  (observed-related-to ?o ?where))
                          )
                    )
)

)
